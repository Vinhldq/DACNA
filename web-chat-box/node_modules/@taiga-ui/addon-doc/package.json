      "esm": "./esm2022/types/taiga-ui-addon-doc-types.mjs",
      "default": "./fesm2022/taiga-ui-addon-doc-types.mjs"
    },
    "./utils": {
      "types": "./utils/index.d.ts",
      "esm2022": "./esm2022/utils/taiga-ui-addon-doc-utils.mjs",
