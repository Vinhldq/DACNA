{
  "name": "@taiga-ui/addon-doc",
  "version": "4.28.0",
  "description": "Taiga UI based library for developing documentation portals for Angular libraries.",
  "keywords": [
    "angular",
    "component",
    "library",
    "docs",
    "documentation",
    "storybook"
  ],
  "homepage": "https://github.com/taiga-family/taiga-ui",
  "repository": "https://github.com/taiga-family/taiga-ui",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@angular/cdk": ">=16.0.0",
    "@angular/common": ">=16.0.0",
    "@angular/core": ">=16.0.0",
    "@angular/forms": ">=16.0.0",
    "@angular/router": ">=16.0.0",
    "@ng-web-apis/common": "^4.11.1",
    "@taiga-ui/addon-mobile": "^4.28.0",
    "@taiga-ui/cdk": "^4.28.0",
    "@taiga-ui/core": "^4.28.0",
    "@taiga-ui/kit": "^4.28.0",
    "@taiga-ui/legacy": "^4.28.0",
    "@taiga-ui/polymorpheus": "^4.8.0",
    "markdown-it": "^14.1.0",
    "ngx-highlightjs": "^10.0.0"
  },
  "module": "fesm2022/taiga-ui-addon-doc.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
